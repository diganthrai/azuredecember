{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "betadevdatafactory"
		},
		"adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adlsgen2'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://betadevvault.vault.azure.net/"
		},
		"adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://betadevdata.dfs.core.windows.net/"
		},
		"devSqlServer_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "betaserver.database.windows.net"
		},
		"devSqlServer_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dev_db"
		},
		"devSqlServer_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "betaadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "betadevdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adlsgen2_accountKey": {
						"value": "[parameters('adlsgen2_accountKey')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('adlsgen2_properties_typeProperties_url')]"
					},
					"devSqlServer_properties_typeProperties_server": {
						"value": "[parameters('devSqlServer_properties_typeProperties_server')]"
					},
					"devSqlServer_properties_typeProperties_database": {
						"value": "[parameters('devSqlServer_properties_typeProperties_database')]"
					},
					"devSqlServer_properties_typeProperties_userName": {
						"value": "[parameters('devSqlServer_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}